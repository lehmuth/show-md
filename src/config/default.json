{
    "port": 56657,
    "stylesheetName": "github",
    "language": "en",
    "defaultFile": "README.md",
    "includeExtensions": [
        "html",
        "md",
        "txt"
    ],
    "stylesheets": [
        ["default", "{PUBLIC}/css/default.css"],
        ["github", "{PUBLIC}/css/github.css"],
        ["none", "{PUBLIC}/css/none.css"]
    ],
    "pathVariables": [
        ["LOGS", "{APP_ROOT}/logs"]
    ],
    "htDirs": [
        "{SERVER_ROOT}",
        "{HTDOCS}"
    ],
    "errorFiles": [
        [403, "{HTDOCS}/error/error403.md"],
        [404, "{HTDOCS}/error/error404.md"],
        [500, "{HTDOCS}/error/error500.md"]
    ]
}